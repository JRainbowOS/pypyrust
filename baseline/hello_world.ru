fn hello() {
    println!("hello world!");
;
}

fn hello_override_end() {
    print!("hello ");
;
    print!("world");
;
    println!();
;
}

fn hello_multi() {
    print!("hello");
print!(" ");
println!("world");
;
}

fn hello_multi_override_sep() {
    print!("hello");
print!("_");
println!("world");
;
}

